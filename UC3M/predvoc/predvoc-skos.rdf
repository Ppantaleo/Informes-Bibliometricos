    <!-- http://purl.org/predvoc/ -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <dc:creator>Grupo UC3M</dc:creator>
        <dc:description xml:lang="es">Vocabulario SKOS bilingüe para identificación de prácticas editoriales depredadoras en revistas científicas</dc:description>
        <dc:publisher>Universidad Carlos III de Madrid</dc:publisher>
        <dc:title xml:lang="en">PredVoc: Controlled Vocabulary of Predatory Publishing Indicators</dc:title>
        <dc:title xml:lang="es">PredVoc: Vocabulario Controlado de Indicadores de Publicación Depredadora</dc:title>
        <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-07</dcterms:created>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-08</dcterms:modified>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/afiliaciones-contacto"/>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/indexacion-metricas"/>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/integridad-editorial"/>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/practicas-comerciales"/>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/revision-pares"/>
        <skos:hasTopConcept rdf:resource="http://purl.org/predvoc/sitio-web"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/aceptacion-inmediata -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/aceptacion-inmediata">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">acceptance without review</skos:altLabel>
        <skos:altLabel xml:lang="es">aceptación sin revisión</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/revision-pares"/>
        <skos:definition xml:lang="en">Acceptance of manuscripts in impossibly short timeframes, typically less than 7 days</skos:definition>
        <skos:definition xml:lang="es">Aceptación de manuscritos en plazos imposiblemente cortos, típicamente menos de 7 días</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">aceptación inmediata</skos:prefLabel>
        <skos:prefLabel xml:lang="en">immediate acceptance</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/revision-inexistente"/>
        <skos:related rdf:resource="http://purl.org/predvoc/tiempo-revision-sospechoso"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/afiliacion-falsa -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/afiliacion-falsa">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">afiliación inventada</skos:altLabel>
        <skos:altLabel xml:lang="en">invented affiliation</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/afiliaciones-contacto"/>
        <skos:definition xml:lang="es">Afirmación de estar afiliada a instituciones académicas prestigiosas sin evidencia verificable</skos:definition>
        <skos:definition xml:lang="en">Claim to be affiliated with prestigious academic institutions without verifiable evidence</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">afiliación institucional falsa</skos:prefLabel>
        <skos:prefLabel xml:lang="en">false institutional affiliation</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/comite-editorial-ficticio"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/afiliacion-indexadores-falsa -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/afiliacion-indexadores-falsa">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">invented membership</skos:altLabel>
        <skos:altLabel xml:lang="es">membresía inventada</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/indexacion-metricas"/>
        <skos:definition xml:lang="es">Afirmación falsa de ser miembro o estar afiliado a organizaciones de indexación legítimas</skos:definition>
        <skos:definition xml:lang="en">False claim of being a member or affiliated with legitimate indexing organizations</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">afiliación falsa a indexadores</skos:prefLabel>
        <skos:prefLabel xml:lang="en">false indexer affiliation</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/indexacion-falsa"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/afiliaciones-contacto -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/afiliaciones-contacto">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:definition xml:lang="es">Indicadores sobre verificabilidad de afiliaciones institucionales y datos de contacto</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="en">Affiliations and Contact</skos:prefLabel>
        <skos:prefLabel xml:lang="es">Afiliaciones y Contacto</skos:prefLabel>
        <skos:topConceptOf rdf:resource="http://purl.org/predvoc/"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/apartado-postal-sospechoso -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/apartado-postal-sospechoso">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">casilla postal dudosa</skos:altLabel>
        <skos:altLabel xml:lang="en">dubious mailbox</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/afiliaciones-contacto"/>
        <skos:definition xml:lang="en">Exclusive use of PO box without real or verifiable physical address</skos:definition>
        <skos:definition xml:lang="es">Uso exclusivo de apartado postal sin dirección física real o verificable</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">apartado postal sospechoso</skos:prefLabel>
        <skos:prefLabel xml:lang="en">suspicious PO box</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/direccion-falsa"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/apc-excesivos -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/apc-excesivos">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">costos desproporcionados</skos:altLabel>
        <skos:altLabel xml:lang="en">disproportionate costs</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/practicas-comerciales"/>
        <skos:definition xml:lang="en">Charging disproportionately high Article Processing Charges in relation to the editorial service provided</skos:definition>
        <skos:definition xml:lang="es">Cobro de Article Processing Charges desproporcionadamente altos en relación al servicio editorial proporcionado</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">APC excesivos</skos:prefLabel>
        <skos:prefLabel xml:lang="en">excessive APC</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/apc-ocultos -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/apc-ocultos">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">cargos no revelados</skos:altLabel>
        <skos:altLabel xml:lang="en">undisclosed charges</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/practicas-comerciales"/>
        <skos:definition xml:lang="es">La revista no informa claramente sobre los Article Processing Charges antes de la aceptación del manuscrito</skos:definition>
        <skos:definition xml:lang="en">The journal does not clearly inform about Article Processing Charges before manuscript acceptance</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">APC ocultos</skos:prefLabel>
        <skos:prefLabel xml:lang="en">hidden APC</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/cargos-sorpresa"/>
        <skos:related rdf:resource="http://purl.org/predvoc/falta-transparencia"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/ausencia-doble-ciego -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/ausencia-doble-ciego">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">non-anonymous review</skos:altLabel>
        <skos:altLabel xml:lang="es">revisión no anónima</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/revision-pares"/>
        <skos:definition xml:lang="es">Falta de anonimato en el proceso de revisión cuando la revista afirma usar revisión doble ciego</skos:definition>
        <skos:definition xml:lang="en">Lack of anonymity in the review process when the journal claims to use double-blind review</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="en">absence of double-blind</skos:prefLabel>
        <skos:prefLabel xml:lang="es">ausencia de doble ciego</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/ausencia-politicas -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/ausencia-politicas">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">no editorial policies</skos:altLabel>
        <skos:altLabel xml:lang="es">sin políticas editoriales</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/sitio-web"/>
        <skos:definition xml:lang="es">Falta de políticas claras sobre ética, plagio, conflictos de interés o procedimientos editoriales</skos:definition>
        <skos:definition xml:lang="en">Lack of clear policies on ethics, plagiarism, conflicts of interest or editorial procedures</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="en">absence of policies</skos:prefLabel>
        <skos:prefLabel xml:lang="es">ausencia de políticas</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/falta-transparencia"/>
        <skos:related rdf:resource="http://purl.org/predvoc/informacion-editorial-vaga"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/autoplagio -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/autoplagio">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">content recycling</skos:altLabel>
        <skos:altLabel xml:lang="es">reciclaje contenido</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/integridad-editorial"/>
        <skos:definition xml:lang="es">Publicación repetida del mismo contenido sin declaración de publicación previa</skos:definition>
        <skos:definition xml:lang="en">Repeated publication of the same content without declaration of prior publication</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">autoplagio</skos:prefLabel>
        <skos:prefLabel xml:lang="en">self-plagiarism</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/publicacion-duplicada"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/bases-datos-dudosas -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/bases-datos-dudosas">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">indexadores cuestionables</skos:altLabel>
        <skos:altLabel xml:lang="en">questionable indexers</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/indexacion-metricas"/>
        <skos:definition xml:lang="en">Affiliation with low-quality databases or with lax inclusion criteria</skos:definition>
        <skos:definition xml:lang="es">Afiliación a bases de datos de baja calidad o con criterios de inclusión laxos</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">bases de datos dudosas</skos:prefLabel>
        <skos:prefLabel xml:lang="en">dubious databases</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/indexacion-falsa"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/cargos-sorpresa -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/cargos-sorpresa">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">hidden fees</skos:altLabel>
        <skos:altLabel xml:lang="es">tarifas ocultas</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/practicas-comerciales"/>
        <skos:definition xml:lang="es">Aparición de cargos adicionales no mencionados previamente en el proceso de publicación</skos:definition>
        <skos:definition xml:lang="en">Appearance of additional charges not previously mentioned in the publication process</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">cargos sorpresa</skos:prefLabel>
        <skos:prefLabel xml:lang="en">surprise charges</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/apc-ocultos"/>
        <skos:related rdf:resource="http://purl.org/predvoc/facturacion-fraudulenta"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/citaciones-falsas -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/citaciones-falsas">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">citas fabricadas</skos:altLabel>
        <skos:altLabel xml:lang="en">fabricated citations</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/indexacion-metricas"/>
        <skos:definition xml:lang="en">Inclusion of non-existent or manipulated citations to inflate impact metrics</skos:definition>
        <skos:definition xml:lang="es">Inclusión de citaciones inexistentes o manipuladas para inflar métricas de impacto</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">citaciones falsas</skos:prefLabel>
        <skos:prefLabel xml:lang="en">false citations</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/indices-manipulados"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/comite-editorial-ficticio -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/comite-editorial-ficticio">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">editores falsos</skos:altLabel>
        <skos:altLabel xml:lang="en">fake editors</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/integridad-editorial"/>
        <skos:definition xml:lang="en">Use of academics&apos; names without their consent on the editorial board or invention of non-existent people</skos:definition>
        <skos:definition xml:lang="es">Uso de nombres de académicos sin su consentimiento en el comité editorial o invención de personas inexistentes</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">comité editorial ficticio</skos:prefLabel>
        <skos:prefLabel xml:lang="en">fictitious editorial board</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/afiliacion-falsa"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/conflicto-intereses-oculto -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/conflicto-intereses-oculto">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="es">COI no declarado</skos:altLabel>
        <skos:altLabel xml:lang="en">undeclared COI</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/integridad-editorial"/>
        <skos:definition xml:lang="en">Failure to declare financial or personal relationships that could influence editorial decisions</skos:definition>
        <skos:definition xml:lang="es">Falta de declaración de relaciones financieras o personales que podrían influir en decisiones editoriales</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">conflicto de intereses oculto</skos:prefLabel>
        <skos:prefLabel xml:lang="en">hidden conflict of interest</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/falta-transparencia"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/contenido-contradictorio -->

    <owl:NamedIndividual rdf:about="http://purl.org/predvoc/contenido-contradictorio">
        <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <skos:altLabel xml:lang="en">inconsistent information</skos:altLabel>
        <skos:altLabel xml:lang="es">información inconsistente</skos:altLabel>
        <skos:broader rdf:resource="http://purl.org/predvoc/sitio-web"/>
        <skos:definition xml:lang="en">Presence of contradictory or inconsistent information in different sections of the website</skos:definition>
        <skos:definition xml:lang="es">Presencia de información contradictoria o inconsistente en diferentes secciones del sitio web</skos:definition>
        <skos:inScheme rdf:resource="http://purl.org/predvoc/"/>
        <skos:prefLabel xml:lang="es">contenido contradictorio</skos:prefLabel>
        <skos:prefLabel xml:lang="en">contradictory content</skos:prefLabel>
        <skos:related rdf:resource="http://purl.org/predvoc/informacion-editorial-vaga"/>
    </owl:NamedIndividual>
    


    <!-- http://purl.org/predvoc/descuentos-agresivos -->

